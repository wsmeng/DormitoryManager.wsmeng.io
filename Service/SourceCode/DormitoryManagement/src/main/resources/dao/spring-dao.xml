    </bean>
    
    <bean id="TBManageDao"
        class="com.wsm.DormitoryManagement.daoImpl.TBManageDaoImpl">
        
        <property name="dataSource" ref="dataSource"/>
        
    </bean>
    
    <bean id="LogDao"
