    </bean>
    
    <bean id="DormitoryDao"
        class="com.wsm.DormitoryManagement.daoImpl.DormitoryDaoImpl">
        
        <property name="dataSource" ref="dataSource"/>
        
    </bean>
    
    <bean id="StudentDao"
        class="com.wsm.DormitoryManagement.daoImpl.StudentDaoImpl">
        
        <property name="dataSource" ref="dataSource"/>
        
    </bean>
    
    <bean id="TeacherDao"
        class="com.wsm.DormitoryManagement.daoImpl.TeacherDaoIpl">
        
